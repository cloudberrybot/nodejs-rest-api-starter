{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf ../../dist",
    "build": "npm run clean && tsc --project tsconfig.json && cp package.json package-lock.json ../../dist && cp -R src/generated ../../dist && rsync -av --exclude='*.ts' prisma ../../dist",
    "start": "npx prisma generate && node ./src/app.js",
    "studio": "npx prisma studio",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "zip": "cd dist && zip -r ../../../api.zip . -x '*.git*' -x '*node_modules*'",
    "db:migrate": "npx prisma migrate dev --name init",
    "db:seed": "npx ts-node ./prisma/seed.ts",
    "swagger": "ts-node ./src/swagger.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@types/express": "^4.17.21",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.2",
    "@commitlint/config-conventional": "^18.4.2",
    "@types/pg": "^8.10.9",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.0",
    "typescript": "^5.2.2"
  }
}
